var radNw="",radSite="",radCont="",radType="",radPos="",radCharsetParam="",radSslFlg="-1";
function radvertisesc(c){var a="";for(key in c.params){var b=c.params[key];"rad_nw"==key?(a+="rad_nw="+encodeURIComponent(b)+"&",radNw=b):"rad_site"==key?(a+="rad_site="+encodeURIComponent(b)+"&",radSite=b):"rad_cont"==key?(a+="rad_cont="+encodeURIComponent(b)+"&",radCont=b):"rad_type"==key?(a+="rad_type="+encodeURIComponent(b)+"&",radType=b):"rad_pos"==key?(a+="rad_pos="+encodeURIComponent(b)+"&",radPos=b):"rad_format"==key?a+="rad_format="+encodeURIComponent(b)+"&":"rad_ftype"==key?a+="rad_ftype="+
encodeURIComponent(b)+"&":"rad_month"==key?a+="rad_month="+encodeURIComponent(b)+"&":"rad_charset"==key?(a+="rad_charset="+encodeURIComponent(b)+"&",radCharsetParam=b):"rad_frame"==key?a+="rad_frame="+encodeURIComponent(b)+"&":"rad_ssl"==key?radSslFlg=b:"rad_shuffle"==key?a+="rad_shuffle="+encodeURIComponent(b)+"&":"rad_tg"==key&&(a+="rad_tg="+encodeURIComponent(b)+"&")}a+="rad_script="+encodeURIComponent(0);a+="&_radvjs=1";try{c="http:";"https:"==location.protocol&&(c="https:");switch(radSslFlg){case "0":c=
"http:";break;case "1":c="https:"}if("mass"==radType){var d=c+"//grp10.ias.rakuten.co.jp/innerBodyMassAdv/?"+a,e='<script language="JavaScript1.1" charset="'+rgetCharset(radCharsetParam)+'" src="'+d+'">\x3c/script>';document.write(e)}else{d=c+"//grp10.ias.rakuten.co.jp/innerBodyAdv/?"+a;a=radNw+"_"+radSite+"_"+radCont+"_"+radType+"_"+radPos;getLastRadvertiseScript().id=a;var f=rgetCharset(radCharsetParam);replaceDocumentWrite(d,f)}}catch(g){return null}}
function getLastRadvertiseScript(){for(var c=document.getElementsByTagName("script"),a=c.length-1;0<=a;){var b=c[a],d=b.text.replace("\r\n","\n").replace("\n","");if(0<=d.indexOf("radvertisesc")&&0<=d.indexOf("rad_nw"))return b;a--}}function replaceDocumentWrite(c,a){var b=document.createElement("script");b.src=c;b.charset=a;var d=document.getElementsByTagName("script")[0];d.parentNode.insertBefore(b,d)}
function radvertisescwrite(c,a){if(null==a)document.write(c);else{var b=document.getElementById(a),d=document.createElement("div");d.innerHTML=c;for(var e=d.childNodes.length,f=document.createDocumentFragment(),g=0;g<e;g++)f.appendChild(d.childNodes[0]);b.parentNode.insertBefore(f,b)}}
function radvertiseakwrite(c,a){try{var b="http:";"https:"==location.protocol&&(b="https:");switch(radSslFlg){case "0":b="http:";break;case "1":b="https:"}var b=b+"//image.ias.rakuten.co.jp/massadv/"+(radNw+"/"+radSite+"/"+radCont+"/"+radType+"/"+radPos+"/"+c+"/"+c+"_"+a+".html"),d='<script language="JavaScript1.1" charset="'+rgetCharset(radCharsetParam)+'" src="'+b+'">\x3c/script>';document.write(d)}catch(e){return null}}
function rgetCharset(c){return c.match(/^Shift_JIS$/i)?"Shift_JIS":c.match(/^EUC-JP$/i)?"EUC-JP":"UTF-8"};